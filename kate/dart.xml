<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE language SYSTEM "language.dtd">
<language name="Dart" version="1" kateversion="5.0" section="Sources" extensions="*.dart"
          mimetype="text/x-dart;text/x-dart-source;text/x-script.dart;application/x-dart" indenter="cstyle"
          author="Herman Bergwerf (hermanbergwerf@gmail.com)" license="CC0">
  <highlighting>
    <list name="control">
      <item>await</item>
      <item>break</item>
      <item>case</item>
      <item>catch</item>
      <item>continue</item>
      <item>default</item>
      <item>do</item>
      <item>else</item>
      <item>finally</item>
      <item>for</item>
      <item>if</item>
      <item>rethrow</item>
      <item>return</item>
      <item>switch</item>
      <item>throw</item>
      <item>try</item>
      <item>while</item>
      <item>yield</item>
      <item>yield*</item>
    </list>
    <list name="keywords">
      <item>abstract</item>
      <item>as</item>
      <item>assert</item>
      <item>async</item>
      <item>async*</item>
      <item>class</item>
      <item>const</item>
      <item>covariant</item>
      <item>deferred</item>
      <item>dynamic</item>
      <item>enum</item>
      <item>export</item>
      <item>external</item>
      <item>extends</item>
      <item>factory</item>
      <item>false</item>
      <item>final</item>
      <item>get</item>
      <item>implements</item>
      <item>import</item>
      <item>in</item>
      <item>is</item>
      <item>library</item>
      <item>new</item>
      <item>null</item>
      <item>operator</item>
      <item>part</item>
      <item>set</item>
      <item>static</item>
      <item>super</item>
      <item>sync*</item>
      <item>this</item>
      <item>true</item>
      <item>typedef</item>
      <item>var</item>
      <item>void</item>
      <item>with</item>
    </list>
    <contexts>
      <context attribute="Normal Text" lineEndContext="#pop" name="Normal Text">
        <keyword attribute="Keyword" context="#stay" String="keywords" />
        <keyword attribute="Control" context="#stay" String="control" />
        
        <Float attribute="Float" context="NoRegExp" />
        <Int attribute="Decimal" context="NoRegExp" />
        
        <!-- Strings with single or double quotes -->
        <DetectChar attribute="String" context="string" char="&quot;" />
        <DetectChar attribute="String" context="string-sq" char="'" />
      </context>
      
      <!-- String contexts -->
      <context attribute="String" lineEndContext="#stay" name="string">
        <DetectChar attribute="String" context="#pop" char="&quot;" />
      </context>
      <context attribute="String" lineEndContext="#stay" name="string-sq">
        <DetectChar attribute="String" context="#pop" char="'" />
      </context>
    </contexts>
    <itemDatas>
      <itemData name="Normal Text"  defStyleNum="dsNormal" spellChecking="false" />
      <itemData name="Keyword"      defStyleNum="dsKeyword" spellChecking="false" />
      <itemData name="Control"      defStyleNum="dsControlFlow" spellChecking="false" />
      <itemData name="String"       defStyleNum="dsString" spellChecking="false" />
      <itemData name="Decimal"      defStyleNum="dsDecVal" spellChecking="false" />
      <itemData name="Float"        defStyleNum="dsFloat" spellChecking="false" />
      <itemData name="Comment"      defStyleNum="dsComment" />
    </itemDatas>
  </highlighting>

  <general>
    <comments>
      <comment name="singleLine" start="//" />
      <comment name="multiLine" start="/*" end="*/" />
    </comments>
  </general>
</language>

